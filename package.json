		"husky": "^9.1.7",
		"knip": "^5.47.0",
		"lint-staged": "^15.5.0",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.0"
	},
	"lint-staged": {
		"*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"packageManager": "yarn@3.6.1"
}
