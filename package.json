	"devDependencies": {
		"@eslint/js": "^9.16.0",
		"@next/eslint-plugin-next": "^15.2.4",
		"@stylistic/eslint-plugin": "^4.2.0",
		"@svgr/webpack": "^8.0.1",
		"@types/mdx": "^2.0.12",
		"@types/react": "^19.0.8",
		"cross-env": "^7.0.3",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-prefer-function-component": "^4.0.1",
		"eslint-plugin-spellcheck": "^0.0.20",
